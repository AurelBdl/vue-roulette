{
  "name": "vue-roulette",
  "version": "0.1.0",
  "type": "module",
  "description": "Vue 3 roulette component",
  "private": false,
  "scripts": {
    "dev": "vite --config vite.demo.config.ts",
    "build:lib": "vite build --config vite.lib.config.ts",
    "build:demo": "vite build --config vite.demo.config.ts",
    "build": "npm run build:lib && npm run build:demo",
    "preview": "vite preview --config vite.demo.config.ts"
  },
  "main": "dist/vue-roulette.umd.cjs",
  "module": "dist/vue-roulette.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-roulette.js",
      "require": "./dist/vue-roulette.umd.cjs"
    },
    "./VueRoulette.vue": "./src/components/VueRoulette.vue"
  },
  "files": [
    "dist",
    "src/components/VueRoulette.vue",
    "src/index.ts"
  ],
  "keywords": [
    "vue",
    "roulette",
    "component",
    "casino"
  ],
  "author": "AurelBdl",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "vue": "^3.5.24",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}